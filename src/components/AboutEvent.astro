---
import AboutMe from "./AboutList";

import InstaImage from "../assets/image/instagram-color.svg";
import TwittImage from "../assets/image/x-color.svg";
import LinkeImage from "../assets/image/linkedin-color.svg";
import ReadcImage from "../assets/profileimage.jpg";

export const frontmatter = {
  title: 'About',
};
---

<section class="w-full md:w-[48rem] block mx-auto mt-5 mb-8" id="about">
    <AboutMe
      title={"Instagram"}
      subtitle={". Just now"}
      content={
        "My other skills? you can visit my Instagram. You can see Instagram Typography content and some of my Photography Collections."
      }
      imgabout={InstaImage}
      sociallink={"https://www.instagram.com/setografi"}
    />

    <AboutMe
      title={"Twitter"}
      subtitle={". 1m ago"}
      content={
        "You can see me here sharing design thoughts and sometimes I tell you about my daily activities and hobbies."
      }
      imgabout={TwittImage}
      sociallink={"https://twitter.com/sicalondktr"}
    />

    <AboutMe
      title={"LinkedIn"}
      subtitle={". 3d ago"}
      content={"See my experience and qualifications."}
      imgabout={LinkeImage}
      sociallink={
        "https://www.linkedin.com/in/wahyu-seto-pranata-553719262"
      }
    />

    <AboutMe
      title={"read.cv"}
      subtitle={". 1y ago"}
      content={"Find me and take me."}
      imgabout={ReadcImage}
      sociallink={"https://read.cv/setografi"}
    />
</section>
